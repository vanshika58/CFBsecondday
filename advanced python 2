#filter 
'''def greater_5(num) :
    if num>5:
        return True
    else:
        return False
g10= lambda num: num>10
l=[1,2,3,14,5,4,6,78]
print(list(filter(greater_5,l)))
print(list(filter(g10,l)))'''

#reduce
'''from functools import reduce
sum=lambda a,b: a+b
l=[1,2,3,4,5,6]
val= reduce(sum,l)
print(val)'''

