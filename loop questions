   print (i)

#step size in python
for i in range(1,8,2): #here 2 is step size i.e numbers are print with gape of 2
    print(i)
 #for with else loop
for i in range(10):
   print(i)
else:
    print("this is inside else of for") #it runs when condition of for loop is false

#break statment
for i in range(10):
   print(i)
   if i==6:
    break
else:
    print("this is inside else of for") #it runs when condition of for loop is false

